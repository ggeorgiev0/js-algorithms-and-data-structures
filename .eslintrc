{
    "root": true,
    "extends": ["airbnb", "prettier"],
    "plugins": ["jest"],
    "env": {
      "jest/globals": true
    },
    "rules": {
      "prefer-destructuring": "warn",
      "no-bitwise": "off",
      "no-lonely-if": "off",
      "class-methods-use-this": "off",
      "arrow-body-style": "off",
      "no-loop-func": "off"
    }
  }
  